<section class="end_users_all">
<div class="container">
  <div class="row">
  <div class="col-xs-offset-2">
  	<p>会員一覧</p>
  </div>
</div>
  <br>
  <div class="col-xs-8 col-xs-offset-2">
    <table class='table table-hover table-inverse'>
      <thead>
        <tr> 
          <th>会員ID</th>
          <th>氏名</th>
          <th>メールアドレス</th>
          <th>ステータス</th>
        </tr>
      </thead>

      <tbody>
        <% @end_users.each do |end_user| %>
          <tr>
            <td><%= end_user.id %>
            </td>
            <td><%= end_user.first_name %><%= end_user.last_name %></td>
            <td><%= end_user.email %></td>
            <% if end_user.deleted_at != nil %>
            <td><%= "退会済み" %></td>
            <% else %>
            <td><%= "有効" %>
            <% end %>

          </tr>
        <% end %>
      </tbody>
    </table>
  </div>